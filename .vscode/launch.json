    "version": "0.2.0",
    "configurations": [        
        {
            "type": "node",
            "request": "attach",
            "name": "Attach by Process ID",
            "processId": "${command:PickProcess}"
        },
        {
            "name": "Launch download-sheets",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/src/jobs/download-sheets/index.js"
        },
        {
            "name": "Launch row-processing",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/src/jobs/row-processing/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch process-row-cells.js",
            "program": "${workspaceFolder}/process-row-cells.js"
        }
    ]
}